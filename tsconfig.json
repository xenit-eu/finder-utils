{
    "compilerOptions": {
        "outDir": "./dist",
        "target": "es5",
        "module": "commonjs",
        "moduleResolution": "node",
        "isolatedModules": false,
        "declaration": true,
        "noImplicitAny": true,
        "noImplicitUseStrict": true,
        "preserveConstEnums": true,
        "strictNullChecks": true,
    }
}
